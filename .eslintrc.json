{
  "extends": ["next", "plugin:jsx-a11y/recommended"],
  "root": true,
  "rules": {
    "react/no-unescaped-entities": 0,
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": ["Link"],
        "specialLink": ["hrefLeft", "hrefRight"],
        "aspects": ["invalidHref"]
      }
    ]
  }
}